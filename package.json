{
  "name": "express-api-template",
  "version": "0.0.1",
  "description": "Express APIs Template",
  "main": "dist/server.js",
  "repository": "git@github.com:houseoftypescript/express-api-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "engines": {
    "node": "20.1.0",
    "npm": "9.6.6",
    "yarn": "1.22.19"
  },
  "scripts": {
    "prebuild": "rimraf dist && yarn docs:swagger:html",
    "build": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "start:prod": "node .",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "docs:swagger:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Express.js - API - Template'",
    "lint": "eslint . --fix --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.ts",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "http-errors": "2.0.0",
    "morgan": "1.10.0",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "tsoa": "5.1.1"
  },
  "devDependencies": {
    "@redocly/cli": "1.0.0-beta.125",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.16.5",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@webpack-cli/generators": "3.0.3",
    "concurrently": "8.0.1",
    "eslint": "8.40.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.22",
    "openapi-to-postmanv2": "4.12.0",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "prisma": "4.13.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "webpack": "5.82.0",
    "webpack-cli": "5.1.0",
    "webpack-node-externals": "3.0.0"
  }
}
